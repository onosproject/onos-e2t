-- **************************************************************
--
-- Interface Elementary Procedure Class
--
-- **************************************************************

E2AP-ELEMENTARY-PROCEDURE-VONE ::= CLASS {
	&InitiatingMessageVone										,
	&SuccessfulOutcomeVone							OPTIONAL	,
	&UnsuccessfulOutcomeVone						OPTIONAL	,
	&procedureCode				ProcedureCodeVone	UNIQUE	,
	&criticality				CriticalityVone		DEFAULT ignore
}

WITH SYNTAX {
	INITIATING MESSAGE			&InitiatingMessageVone
	[SUCCESSFUL OUTCOME			&SuccessfulOutcomeVone]
	[UNSUCCESSFUL OUTCOME		&UnsuccessfulOutcomeVone]
	PROCEDURE CODE				&procedureCode
	[CRITICALITY				&criticality]
}

-- **************************************************************
--
-- Interface PDU Definition
