-- **************************************************************

E2AP-PDU-VONE ::= CHOICE {
	initiatingMessage			InitiatingMessageVone,
	successfulOutcome			SuccessfulOutcomeVone,
	unsuccessfulOutcome			UnsuccessfulOutcomeVone,
	...
}

InitiatingMessageVone ::= SEQUENCE {
	procedureCode	E2AP-ELEMENTARY-PROCEDURE-VONE.&procedureCode		({E2AP-ELEMENTARY-PROCEDURES-VONE}),
	criticality		E2AP-ELEMENTARY-PROCEDURE-VONE.&criticality			({E2AP-ELEMENTARY-PROCEDURES-VONE}{@procedureCode}),
	value			E2AP-ELEMENTARY-PROCEDURE-VONE.&InitiatingMessageVone	({E2AP-ELEMENTARY-PROCEDURES-VONE}{@procedureCode})
}

SuccessfulOutcomeVone ::= SEQUENCE {
	procedureCode	E2AP-ELEMENTARY-PROCEDURE-VONE.&procedureCode		({E2AP-ELEMENTARY-PROCEDURES-VONE}),
	criticality		E2AP-ELEMENTARY-PROCEDURE-VONE.&criticality			({E2AP-ELEMENTARY-PROCEDURES-VONE}{@procedureCode}),
	value			E2AP-ELEMENTARY-PROCEDURE-VONE.&SuccessfulOutcomeVone	({E2AP-ELEMENTARY-PROCEDURES-VONE}{@procedureCode})
